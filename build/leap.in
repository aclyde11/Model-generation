source leaprc.protein.ff14SBonlysc
source leaprc.gaff
set default PBRadii mbondi3
rec = loadPDB build/apo.pdb # May need full filepath?
saveAmberParm rec build/apo.prmtop build/apo.inpcrd
lig = loadmol2 build/lig.mol2
loadAmberParams build/lig.frcmod
com = combine {rec lig}
saveAmberParm lig build/lig.prmtop build/lig.inpcrd
saveAmberParm com build/com.prmtop build/com.inpcrd
quit
